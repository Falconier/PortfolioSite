                                var three = Number(document.getElementById("v3").value);
                                var four = Number(document.getElementById("v4").value);
                                var five = Number(document.getElementById("v5").value);

                                var min = Math.min(one, two, three, four, five);
                                var max = Math.max(one, two, three, four, five);
                                var sum = one + two + three + four + five;
                                var avg = sum / 5;
                                var product = one * two * three * four * five;

                                document.getElementById("list2").innerHTML = "Min: " + min + " Max: " + max + " Average: " + avg + " Sum: " + sum + " Product: " + product;
                                }
